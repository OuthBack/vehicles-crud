{
  "name": "vehicle-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm prisma:generate && pnpm generate:docs && next build",
    "dev": "pnpm prisma:generate && next dev",
    "generate:docs": "ts-node -r tsconfig-paths/register  src/scripts/swagger.script.ts",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "populate": "ts-node src/scripts/vehicle.script.ts",
    "prepare": "husky install",
    "prisma:generate": "prisma generate",
    "prisma:pull": "prisma db pull",
    "prisma:push": "prisma db push",
    "prisma:web": "pnpm prisma:generate && pnpm prisma:pull && prisma studio",
    "start": "next start",
    "test": "NODE_ENV=test mocha --config .mocharc.cjs"
  },
  "dependencies": {
    "@prisma/client": "^4.14.0",
    "@t3-oss/env-nextjs": "^0.4.0",
    "next": "^13.4.2",
    "next-swagger-doc": "^0.4.0",
    "nextjs-cors": "^2.1.2",
    "openapi3-ts": "^4.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sinon": "^15.1.0",
    "superjson": "1.12.2",
    "swagger-autogen": "^2.23.1",
    "swagger-ui-react": "^4.19.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/eslint": "^8.37.0",
    "@types/expect": "^24.3.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.16.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/sinon": "^10.0.15",
    "@types/swagger-ui-react": "^4.18.0",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "chai": "^4.3.7",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.2",
    "eslint-plugin-jsdoc": "^46.1.0",
    "husky": "^8.0.3",
    "mocha": "^10.2.0",
    "prisma": "^4.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.13.1"
  }
}